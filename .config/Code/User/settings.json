    "git.allowForcePush": true,
    "window.menuBarVisibility": "toggle",
    "diffEditor.ignoreTrimWhitespace": false,
    "redhat.telemetry.enabled": false,
    "python.pythonPath": "/home/shvix/.pyenv/shims/python",
    "[go]": {

        "editor.insertSpaces": false,
        "editor.formatOnSave": false,
        "editor.codeActionsOnSave": {
            "source.organizeImports": false
        },
        "editor.suggest.snippetsPreventQuickSuggestions": false
    },
    "gitlens.codeLens.scopes": [
        "document",
        "containers"
    ],
    "gitlens.currentLine.pullRequests.enabled": false,
    "gitlens.hovers.enabled": false,
    "gitlens.views.commits.files.layout": "tree",
    "gitlens.views.commits.files.compact": false,
    "gitlens.views.formats.commits.label": "${tips -> }${message}",
    "gitlens.views.repositories.showCommits": false,
    "gitlens.views.repositories.files.layout": "tree",
    "gitlens.views.repositories.files.compact": false,
    "gitlens.views.fileHistory.files.layout": "tree",
    "gitlens.views.repositories.showBranches": false,
    "gitlens.views.repositories.showRemotes": false,
    "gitlens.views.repositories.showStashes": false,
    "gitlens.views.repositories.showTags": false,
    "gitlens.views.repositories.showContributors": false,
    "gitlens.views.branches.files.layout": "tree",
    "gitlens.views.branches.files.compact": false,
    "gitlens.views.remotes.files.compact": false,
    "gitlens.views.remotes.files.layout": "tree",
    "gitlens.views.stashes.files.layout": "tree",
    "gitlens.views.stashes.files.compact": false,
    "gitlens.views.tags.files.layout": "tree",
    "gitlens.views.tags.files.compact": false,
    "gitlens.views.contributors.files.layout": "tree",
    "gitlens.views.contributors.files.compact": false,
    "gitlens.views.searchAndCompare.files.layout": "tree",
    "gitlens.views.searchAndCompare.files.compact": false,
    "gitlens.blame.toggleMode": "window",
    "gitlens.blame.highlight.locations": [
        "overview",
        "line"
    ],
    "gitlens.changes.toggleMode": "window",
    "gitlens.changes.locations": [
        "overview"
    ],
    "gitlens.heatmap.toggleMode": "window",
    "gitlens.heatmap.locations": [
        "overview",
        "gutter"
    ],
    "gitlens.menus": {
        "editorGroup": {
            "blame": true,
            "compare": true
        },
        "editorTab": {
            "clipboard": true,
            "compare": true,
            "history": true,
            "remote": true
        },
        "explorer": {
            "clipboard": true,
            "compare": true,
            "history": true,
            "remote": true
        },
        "scm": {
            "authors": true
        },
        "scmGroupInline": {
            "stash": true
        },
        "scmGroup": {
            "compare": true,
            "openClose": true,
            "stash": true
        },
        "scmItem": {
            "clipboard": true,
            "compare": true,
            "history": true,
            "remote": false,
            "stash": true
        }
    },
    "gitlens.currentLine.enabled": false,
    "gitlens.codeLens.enabled": false,
    "workbench.enableExperiments": false,
    "update.mode": "start",
    "telemetry.telemetryLevel": "off",
    "extensions.autoUpdate": false,
    "workbench.settings.enableNaturalLanguageSearch": false,
    "python.analysis.completeFunctionParens": true,
    "python.analysis.typeCheckingMode": "strict",
    "python.analysis.logLevel": "Trace",
    "editor.detectIndentation": false,
}